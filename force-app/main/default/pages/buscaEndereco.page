<apex:page standardController="Endereco__c" lightningStylesheets="true">
<h1>Inserir Endereço através do VFP</h1>
<br/>
    <apex:form>
        <apex:pageBlock>
            <apex:pageBlockSection title="Busca de Endereço">
                <apex:inputField value="{!Endereco__c.CEP__c}"/>
                <apex:inputField value="{!Endereco__c.Account__c}"/>
                <apex:commandButton action="{!Save}" value="Salvar"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
    <apex:iframe src="https://www.youtube.com/embed/4l52uwIz2eo"></apex:iframe>
    Hoje é o dia: <p id="output"></p>
    <script>
        document.getElementById('output').innerHTML = Date();
    </script>
</apex:page>